  set img [image create photo]
  $img put $pixels
  for {set i 0} {$i < 4} {incr i} \
  {
    for {set j 0} {$j < 6} {incr j} \
    {
      if {[lindex $pixels $i $j] == "#000"} \
      { $img transparency set $j $i 1 }
    }
  }
  button .b -image $img
  grid .b
  wm geometry . 200x200